<?xml version="1.0" encoding="UTF-8"?>
<plugin>
	<title xml:lang="ko">KCB OkName 플러그인</title>
	<description xml:lang="ko">KCB Okname 플러그인 v376 용</description>
	<version>0.1</version>
	<date>2017-02-03</date>
	<author email_address="root@singleview.co.kr" link="http://singleview.co.kr">
		<name xml:lang="ko">Singleview.co.kr</name>
		<homepage xml:lang="ko">Singleview.co.kr</homepage>
	</author>
	<license link="">Commercial</license>
	<extra_vars>
		<group>
			<title xml:lang="ko">기본설정</title>
			<var name="domain" type="text">
				<title xml:lang="ko">사용 도메인</title>
				<description xml:lang="ko"> 실제 사용 도메인을 입력합니다. (www포함하는 경우 www. 까지 반드시 입력. http:// 제외)</description>
			</var>
			<var name="shop_id" type="text">
				<title xml:lang="ko">샵ID</title>
				<description xml:lang="ko">오케이네임즈에서 발급받은 샵 ID를 입력합니다.</description>
			</var>
		</group>
		<group>
			<title xml:lang="ko">옵션</title>
			<var name="guide_text" type="text">
				<title xml:lang="ko">안내문구</title>
				<description xml:lang="ko">팝업창에 뜰 안내문구를 입력하세요.</description>
			</var>
		</group>
	</extra_vars>
</plugin>