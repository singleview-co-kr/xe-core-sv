        <table name="svpromotion_coupon_list" />
    </tables>
    <columns>
        <column name="regdate" var="regdate" default="curdate()" />
    </columns>
    <conditions>
		<condition operation="equal" column="coupon_srl" var="coupon_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
