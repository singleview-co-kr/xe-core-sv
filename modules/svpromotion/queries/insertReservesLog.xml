<query id="insertReservesLog" action="insert">
	<tables>
		<table name="svpromotion_reserves_log" />
	</tables>
	<columns>
		<column name="reserves_srl" var="reserves_srl" notnull="notnull" />
		<column name="member_srl" var="member_srl" filter="number" notnull="notnull" />
		<column name="order_srl" var="order_srl" filter="number" notnull="notnull" />
		<column name="mode" var="mode" notnull="notnull" />
		<column name="amount" var="amount" notnull="notnull" />
		<column name="is_active" var="is_active" default="N" notnull="notnull" />
		<column name="is_deleted" var="is_deleted" default="N" notnull="notnull" />
		<column name="regdate" var="regdate" default="curdate()" />
	</columns>
</query>
