<query id="insertCrontabLog" action="insert">
	<tables>
		<table name="svestudio_crontab_log" />
	</tables>
	<columns>
		<column name="task" var="task" />
		<column name="param" var="param" />
		<column name="result" var="result" />
		<column name="remote_addr" var="remote_addr" notnull="notnull" />
		<column name="http_user_agent" var="http_user_agent" />
		<column name="regdate" var="regdate" default="curdate()" />
	</columns>
</query>